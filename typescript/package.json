{
  "name": "typescript-apollo-server",
  "version": "1.0.0",
  "description": "experimentation",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --verbose --detectOpenHandles",
    "test:cov": "jest --coverage",
    "start": "npm run build:dist && node 'dist/index.js'",
    "copy-files": "mkdir dist/resources || true && cp src/resources/** dist/resources/",
    "start:dev": "npm run build:dev",
    "build": "nodemon 'src/app/index.ts' --exec 'ts-node' src/index.ts -e ts,graphql",
    "build:dist": "tsc && npm run copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com/sylhare/Apollo.git"
  },
  "keywords": [
    "apollo",
    "graphql",
    "server"
  ],
  "author": "sylhare",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sylhare/Apollo/issues"
  },
  "homepage": "https://github.com/sylhare/Apollo#readme",
  "dependencies": {
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.11",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.14.37",
    "apollo-server-express": "^2.22.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.4",
    "helmet": "^4.4.1",
    "http": "^0.0.1-security",
    "ncp": "^2.0.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "apollo-server-testing": "^2.25.2",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.4"
  }
}
